          <table id="example" class="display table" style="width:100%">
    <thead>
        <tr style="color: rgb(12, 46, 138);">
            <th>Id</th>
            <th>Name</th>
            <th>Ph.No</th>
            <th>City</th>
            <th>Email</th>
            <th>DOB</th>
          
            <th>Delete</th>
            <th>View</th>
        </tr>
    </thead>
    <tbody>
        
         {{#each rows}}
        <tr>
            <th>{{this.rollno}}</th>
            <td>{{this.name}}</td>
            <td>{{this.sm}}</td>
            <td>{{this.city}}</td>
            <td>{{this.email}}</td>
            <td>{{this.dob}}</td>
           
            <td><a href="/deleteuser/{{this.id}}" class="delete btn btn-sm btn-danger b">Delete</a></td>
            <td><a href="/view/{{this.id}}" class="delete btn btn-sm btn-outline-primary b">View</a></td>
        </tr>
        {{/each}} 

    </tbody>
</table>


