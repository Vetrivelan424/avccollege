                <header id="header" class="d-flex align-items-center" style="margin-bottom:20px">
                  <img src="/images/avclogo.png" alt="" style="width: 4.5rem;
    margin-left: 1.5rem">
    <div class="container d-flex justify-content-between" style="margin-left:0px;padding-left:0px">
      <div id="logo">
        <h1><a href="index.html">AVC<span>College</span></a></h1>
        <!-- Uncomment below if you prefer to use an image logo -->
        <!-- <a href="index.html"><img src="assets/img/logo.png" alt=""></a>-->
      </div>
       <nav id="navbar" class="navbar">
        <ul>
          
          <li> <a class="btn btn-dark btn-sm ml-3 pl-2 pr-2 pt-1 pb-1" style="color:white; border-radius: 30px;padding:0"
              href="/">LogOut</a> </li>
        </ul>

        <i class="bi bi-list mobile-nav-toggle"></i>

      </nav><!-- .navbar -->

    </div>
  </header>
          <table id="example" class="display" style="width:100%">
    <thead class="panel">
        <tr style="color: rgb(12, 46, 138);">
            <th>RollNo</th>
            <th>Name</th>
            <th>Ph.No</th>
            <th>City</th>
            <th>Email</th>
            <th>Year</th>
          
            <th>Delete</th>
            <th>View</th>
        </tr>
    </thead>
    <tbody class="panel">
        
         {{#each rows}}
        <tr>
            <th>{{this.rollno}}</th>
            <td>{{this.name}}</td>
            <td>{{this.sm}}</td>
            <td>{{this.city}}</td>
            <td>{{this.email}}</td>
            <td>{{this.year}}</td>
           
            <td><a href="/deleteuser/{{this.id}}" class="delete btn btn-sm btn-outline-danger b">Delete</a></td>
            <td><a href="/view/{{this.id}}" class="delete btn btn-sm btn-outline-primary b">View</a></td>
        </tr>
        {{/each}} 

    </tbody>
</table>
